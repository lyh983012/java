	@Override
	public T plus(T temp) {
		Double T1 = Double.parseDouble(temp.toString());
		Double T2 = Double.parseDouble(this.result.toString());
		Double result=T1+T2;
		this.result=(T)result;
		System.out.println("Result has been saved.");
		return this.result;
	}

	@Override
	public T puls(Object temp, Object temp2) {
		Double T1 = Double.parseDouble(temp.toString());
		Double T2 = Double.parseDouble(temp2.toString());
		Double result=T1+T2;
		this.result=(T)result;
		System.out.println("Result has been saved.");
		return this.result;
	}

